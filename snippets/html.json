{
	"HTML5 Document": {
		"prefix": "html5",
		"body": [
			"<!DOCTYPE html>",
			"<html lang=\"${1:en}\">",
			"<head>",
			"\t<meta charset=\"UTF-8\">",
			"\t<meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">",
			"\t<meta name=\"description\" content=\"${2:Page description}\">",
			"\t<title>${3:Page Title}</title>",
			"</head>",
			"<body>",
			"\t${0}",
			"</body>",
			"</html>"
		],
		"description": "HTML5 document structure"
	},
	"Semantic Header": {
		"prefix": "htmlheader",
		"body": [
			"<header>",
			"\t<nav>",
			"\t\t${0}",
			"\t</nav>",
			"</header>"
		],
		"description": "Semantic HTML header with nav"
	},
	"Semantic Main": {
		"prefix": "htmlmain",
		"body": [
			"<main>",
			"\t${0}",
			"</main>"
		],
		"description": "Semantic HTML main content"
	},
	"Semantic Footer": {
		"prefix": "htmlfooter",
		"body": [
			"<footer>",
			"\t${0}",
			"</footer>"
		],
		"description": "Semantic HTML footer"
	},
	"Semantic Article": {
		"prefix": "htmlarticle",
		"body": [
			"<article>",
			"\t<header>",
			"\t\t<h1>${1:Article Title}</h1>",
			"\t</header>",
			"\t<section>",
			"\t\t${0}",
			"\t</section>",
			"</article>"
		],
		"description": "Semantic HTML article"
	},
	"Semantic Section": {
		"prefix": "htmlsection",
		"body": [
			"<section>",
			"\t<h2>${1:Section Title}</h2>",
			"\t${0}",
			"</section>"
		],
		"description": "Semantic HTML section"
	},
	"Accessible Form": {
		"prefix": "htmlform",
		"body": [
			"<form action=\"${1:#}\" method=\"${2:post}\" novalidate>",
			"\t<fieldset>",
			"\t\t<legend>${3:Form Title}</legend>",
			"\t\t<div>",
			"\t\t\t<label for=\"${4:inputId}\">${5:Label}</label>",
			"\t\t\t<input type=\"${6:text}\" id=\"${4:inputId}\" name=\"${4:inputId}\" required aria-describedby=\"${4:inputId}-error\">",
			"\t\t\t<span id=\"${4:inputId}-error\" class=\"error\" role=\"alert\"></span>",
			"\t\t</div>",
			"\t\t<button type=\"submit\">${7:Submit}</button>",
			"\t</fieldset>",
			"</form>"
		],
		"description": "Accessible HTML form"
	},
	"Accessible Button": {
		"prefix": "htmlbutton",
		"body": [
			"<button type=\"${1:button}\" aria-label=\"${2:Button description}\">${3:Button Text}</button>"
		],
		"description": "Accessible HTML button"
	},
	"Accessible Link": {
		"prefix": "htmllink",
		"body": [
			"<a href=\"${1:#}\" aria-label=\"${2:Link description}\">${3:Link Text}</a>"
		],
		"description": "Accessible HTML link"
	},
	"Accessible Image": {
		"prefix": "htmlimg",
		"body": [
			"<img src=\"${1:image.jpg}\" alt=\"${2:Image description}\" loading=\"${3:lazy}\" width=\"${4:800}\" height=\"${5:600}\">"
		],
		"description": "Accessible HTML image"
	},
	"Accessible Table": {
		"prefix": "htmltable",
		"body": [
			"<table>",
			"\t<caption>${1:Table caption}</caption>",
			"\t<thead>",
			"\t\t<tr>",
			"\t\t\t<th scope=\"col\">${2:Header 1}</th>",
			"\t\t\t<th scope=\"col\">${3:Header 2}</th>",
			"\t\t</tr>",
			"\t</thead>",
			"\t<tbody>",
			"\t\t<tr>",
			"\t\t\t<th scope=\"row\">${4:Row Header}</th>",
			"\t\t\t<td>${5:Data}</td>",
			"\t\t</tr>",
			"\t</tbody>",
			"</table>"
		],
		"description": "Accessible HTML table"
	},
	"Meta Tags": {
		"prefix": "htmlmeta",
		"body": [
			"<meta name=\"${1:name}\" content=\"${2:content}\">"
		],
		"description": "HTML meta tag"
	},
	"Open Graph Meta": {
		"prefix": "htmlog",
		"body": [
			"<meta property=\"og:title\" content=\"${1:Title}\">",
			"<meta property=\"og:description\" content=\"${2:Description}\">",
			"<meta property=\"og:image\" content=\"${3:image.jpg}\">",
			"<meta property=\"og:url\" content=\"${4:https://example.com}\">"
		],
		"description": "Open Graph meta tags"
	},
	"Link Stylesheet": {
		"prefix": "htmllink-css",
		"body": [
			"<link rel=\"stylesheet\" href=\"${1:styles.css}\">"
		],
		"description": "HTML link to stylesheet"
	},
	"Script Tag": {
		"prefix": "htmlscript",
		"body": [
			"<script src=\"${1:script.js}\" defer></script>"
		],
		"description": "HTML script tag"
	},
	"Script Inline": {
		"prefix": "htmlscript-inline",
		"body": [
			"<script>",
			"\t${0}",
			"</script>"
		],
		"description": "HTML inline script"
	},
	"Style Tag": {
		"prefix": "htmlstyle",
		"body": [
			"<style>",
			"\t${0}",
			"</style>"
		],
		"description": "HTML style tag"
	},
	"Details/Summary": {
		"prefix": "htmldetails",
		"body": [
			"<details>",
			"\t<summary>${1:Summary text}</summary>",
			"\t${0}",
			"</details>"
		],
		"description": "HTML details/summary element"
	},
	"Dialog": {
		"prefix": "htmldialog",
		"body": [
			"<dialog id=\"${1:dialogId}\">",
			"\t<form method=\"dialog\">",
			"\t\t<p>${2:Dialog content}</p>",
			"\t\t<button type=\"submit\">${3:Close}</button>",
			"\t</form>",
			"</dialog>"
		],
		"description": "HTML dialog element"
	},
	"Picture Element": {
		"prefix": "htmlpicture",
		"body": [
			"<picture>",
			"\t<source media=\"(min-width: 800px)\" srcset=\"${1:large.jpg}\">",
			"\t<source media=\"(min-width: 400px)\" srcset=\"${2:medium.jpg}\">",
			"\t<img src=\"${3:small.jpg}\" alt=\"${4:Image description}\">",
			"</picture>"
		],
		"description": "HTML picture element for responsive images"
	},
	"Video Element": {
		"prefix": "htmlvideo",
		"body": [
			"<video controls width=\"${1:800}\" height=\"${2:600}\">",
			"\t<source src=\"${3:video.mp4}\" type=\"video/mp4\">",
			"\t<track kind=\"captions\" src=\"${4:captions.vtt}\" srclang=\"${5:en}\" label=\"${6:English}\">",
			"\tYour browser does not support the video tag.",
			"</video>"
		],
		"description": "HTML video element with captions"
	},
	"Audio Element": {
		"prefix": "htmlaudio",
		"body": [
			"<audio controls>",
			"\t<source src=\"${1:audio.mp3}\" type=\"audio/mpeg\">",
			"\tYour browser does not support the audio tag.",
			"</audio>"
		],
		"description": "HTML audio element"
	},
	"SVG Element": {
		"prefix": "htmlsvg",
		"body": [
			"<svg width=\"${1:100}\" height=\"${2:100}\" xmlns=\"http://www.w3.org/2000/svg\">",
			"\t${0}",
			"</svg>"
		],
		"description": "HTML SVG element"
	},
	"Razor Page": {
		"prefix": "razorpage",
		"body": [
			"@page",
			"@model ${1:PageModel}",
			"",
			"@{",
			"\tViewData[\"Title\"] = \"${2:Page Title}\";",
			"}",
			"",
			"<h1>@ViewData[\"Title\"]</h1>",
			"",
			"<p>${0}</p>"
		],
		"description": "ASP.NET Core Razor page"
	},
	"Razor View": {
		"prefix": "razorview",
		"body": [
			"@model ${1:ViewModel}",
			"",
			"@{",
			"\tViewData[\"Title\"] = \"${2:View Title}\";",
			"}",
			"",
			"<h1>@ViewData[\"Title\"]</h1>",
			"",
			"${0}"
		],
		"description": "ASP.NET Core Razor view"
	},
	"Razor Layout": {
		"prefix": "razorlayout",
		"body": [
			"<!DOCTYPE html>",
			"<html lang=\"en\">",
			"<head>",
			"\t<meta charset=\"utf-8\" />",
			"\t<meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />",
			"\t<title>@ViewData[\"Title\"] - ${1:App Name}</title>",
			"\t<link rel=\"stylesheet\" href=\"~/css/site.css\" />",
			"</head>",
			"<body>",
			"\t@RenderBody()",
			"\t<script src=\"~/js/site.js\"></script>",
			"</body>",
			"</html>"
		],
		"description": "ASP.NET Core Razor layout"
	},
	"Razor Section": {
		"prefix": "razorsection",
		"body": [
			"@section ${1:SectionName} {",
			"\t${0}",
			"}"
		],
		"description": "ASP.NET Core Razor section"
	},
	"Razor If": {
		"prefix": "razorif",
		"body": [
			"@if (${1:condition})",
			"{",
			"\t${0}",
			"}"
		],
		"description": "ASP.NET Core Razor if statement"
	},
	"Razor ForEach": {
		"prefix": "razorforeach",
		"body": [
			"@foreach (var ${1:item} in ${2:collection})",
			"{",
			"\t${0}",
			"}"
		],
		"description": "ASP.NET Core Razor foreach loop"
	},
	"Razor For": {
		"prefix": "razorfor",
		"body": [
			"@for (int ${1:i} = 0; ${1:i} < ${2:count}; ${1:i}++)",
			"{",
			"\t${0}",
			"}"
		],
		"description": "ASP.NET Core Razor for loop"
	}
}
